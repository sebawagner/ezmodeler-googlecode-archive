<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="country" name="org.i4change.app.hibernate.beans.adresses.Country">
    <id column="country_id" name="country_id">
      <generator class="increment"/>
    </id>
    <property name="name" type="string" column="name"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="paypal" type="string" column="paypal"/>
  </class>
</hibernate-mapping>
