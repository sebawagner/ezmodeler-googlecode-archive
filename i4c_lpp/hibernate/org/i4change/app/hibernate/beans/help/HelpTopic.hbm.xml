<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="helptopic" name="org.i4change.app.hibernate.beans.help.HelpTopic">
    <id column="helptopic_id" name="helptopicId">
      <generator class="increment"/>
    </id>
    <property name="inserted" type="java.util.Date" column="inserted"/>
    <property name="insertedby" type="long" column="insertedby"/>
    <property name="updated" type="java.util.Date" column="updated"/>
    <property name="updatedby" type="long" column="updatedby"/>
    <property name="helpId" type="long" column="helpId"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="labelId" type="long" column="labelid"/>
    <property name="priority" type="int" column="priority"/>
    <property name="isAgentHelp" type="boolean" column="isagenthelp"/>
    <property name="topicLabelId" type="long" column="topiclabelId"/>
    <property name="helpName" type="string" column="helpname"/>
  </class>
</hibernate-mapping>
