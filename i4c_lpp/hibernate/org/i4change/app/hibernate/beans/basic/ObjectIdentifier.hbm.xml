<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="objectidentifier" name="org.i4change.app.hibernate.beans.basic.ObjectIdentifier">
    <id column="objectidentifier_id" name="objectidentifier_id">
      <generator class="increment"/>
    </id>
    <property name="organization_id" type="long" column="organization_id"/>
    <property name="currentid" type="long" column="currentid"/>
    <property name="updated" type="java.util.Date" column="updated"/>
  </class>
</hibernate-mapping>
