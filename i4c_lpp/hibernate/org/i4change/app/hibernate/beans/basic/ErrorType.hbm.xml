<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="errortypes" name="org.i4change.app.hibernate.beans.basic.ErrorType">
    <id column="errortype_id" name="errortype_id">
      <generator class="increment"/>
    </id>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="label_number" type="long" column="label_number"/>
  </class>
</hibernate-mapping>
