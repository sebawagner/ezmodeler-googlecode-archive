<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="diagram_document" lazy="false" name="org.i4change.app.hibernate.beans.diagram.DiagramDocument">
    <id column="diagram_document_id" name="diagramDocumentId">
      <generator class="increment"/>
    </id>
    <property name="url" type="string" column="url"/>
    <property name="isInternal" type="boolean" column="is_internal"/>
    <property name="fileName" type="string" column="file_name"/>
    <property name="fileIconName" type="string" column="file_icon_name"/>
    <property name="fileHash" type="string" column="filehash"/>
    <property name="diagramObjectId" type="long" column="diagram_object_id"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="inserted" type="java.util.Date" column="inserted"/>
    <property name="insertedBy" type="long" column="insertedby"/>
    <property name="updated" type="java.util.Date" column="updated"/>
    <property name="updatedBy" type="long" column="updatedby"/>
  </class>
</hibernate-mapping>
