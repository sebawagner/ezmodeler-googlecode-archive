<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="diagramobject_propertyhistory" lazy="false" name="org.i4change.app.hibernate.beans.diagram.DiagramObjectPropertyHistory">
    <id column="diagramobject_propertyhistory_id" name="diagramObjectPropertyHistoryId">
      <generator class="increment"/>
    </id>
    <property name="diagramId" type="long" column="diagram_id"/>
    <property name="propertyId" type="long" column="property_id"/>
    <property name="diagramObjectId" type="long" column="diagramobject_id"/>
    <property name="value" type="text" column="value"/>
    <property name="inserted" type="java.util.Date" column="inserted"/>
    <property name="insertedBy" type="long" column="insertedby"/>
  </class>
</hibernate-mapping>
