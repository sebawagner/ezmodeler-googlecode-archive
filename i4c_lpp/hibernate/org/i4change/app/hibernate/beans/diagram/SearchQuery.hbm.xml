<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="searchquery" lazy="false" name="org.i4change.app.hibernate.beans.diagram.SearchQuery">
    <id column="searchquery_id" name="searchQueryId">
      <generator class="increment"/>
    </id>
    <property name="sql" type="text" column="sql_string"/>
    <property name="paramsAsXML" type="text" column="params_as_xml"/>
    <property name="inserted" type="java.util.Date" column="inserted"/>
    <property name="insertedBy" type="long" column="insertedby"/>
    <property name="updated" type="java.util.Date" column="updated"/>
    <property name="updatedBy" type="long" column="updatedby"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="organization_id" type="long" column="organization_id"/>
    <property name="name" type="string" column="name"/>
    <property name="description" type="string" column="description"/>
  </class>
</hibernate-mapping>
