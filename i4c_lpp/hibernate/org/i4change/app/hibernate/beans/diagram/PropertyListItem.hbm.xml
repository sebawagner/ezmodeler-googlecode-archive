<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="propertylistitem" lazy="false" name="org.i4change.app.hibernate.beans.diagram.PropertyListItem">
    <id column="propertylistitem_id" name="propertyListItemId">
      <generator class="increment"/>
    </id>
    <property name="inserted" type="java.util.Date" column="inserted"/>
    <property name="insertedby" type="long" column="insertedby"/>
    <property name="key" type="string" column="key_value"/>
    <property name="value" type="string" column="value_value"/>
    <property name="propertyId" type="long" column="property_id"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="updated" type="java.util.Date" column="updated"/>
  </class>
</hibernate-mapping>
