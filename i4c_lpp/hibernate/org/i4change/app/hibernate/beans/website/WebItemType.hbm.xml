<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="webitemtype" lazy="false" name="org.i4change.app.hibernate.beans.website.WebItemType">
    <id column="webitemtype_id" name="webItemTypeId">
      <generator class="increment"/>
    </id>
    <property name="typeName" type="string" column="typename"/>
    <property name="label_number" type="long" column="label_number"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="insertedBy" type="long" column="insertedby"/>
    <property name="inserted" type="java.util.Date" column="inserted"/>
    <property name="updatedBy" type="java.util.Date" column="updatedBy"/>
    <property name="updated" type="java.util.Date" column="updated"/>
  </class>
</hibernate-mapping>
