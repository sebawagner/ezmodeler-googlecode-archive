    <netRemoteCallRPC name="saveNewDiagramWithProperties" funcname="saveNewDiagramWithProperties" activeErrorHandler="true"
                      remotecontext="$once{  canvas.diagramServiceRPC }" >
        <attribute name="propList" value="null" />
        <handler name="sendCallBack" >
            if (parent.closeAfterUpdate){
