                    //FIXME: See ICH-714 - Fixes to reload the Toolbar depending on read or write access
                    //parent.parent.parent.parent._toolBar.destroy();
                    //parent.parent.parent.parent.initWriteDialogsByDiagram(parent.currentDiagram);
                } else {
                    parent.parent.parent.parent.parent._diagramSideBar._innerSideBar._hierarchicalExplorerSidebar.refresh();
                    parent.parent.parent.parent.parent._diagramSideBar._innerSideBar._processAndActivitiesTableSidebar.refresh();
