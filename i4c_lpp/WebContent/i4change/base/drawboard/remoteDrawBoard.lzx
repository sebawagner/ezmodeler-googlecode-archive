    <method name="doExportByType" args="type">
        this.exportType = type;
        this.generateExportJob.doCall();
    </method>
    
    <method name="doExportByTypeAndTemplate" args="type,pTemplateId,includeText,includeIdeas,includeDetails">
        this.pTemplateId = pTemplateId;
        this.exportType = type;
        this.includeText = includeText;
        this.printIdeas = includeIdeas;
        this.includeDetails = includeDetails;
        if (this.currentDiagram == null) {
        	this.generateExportJob.diagramId = 0;
        } else {
        	this.generateExportJob.diagramId = this.currentDiagram.diagramId;
        }
        this.generateExportJob.doCall();
    </method>
    
    <netRemoteCallRPC name="generateExportJob" funcname="generateExportJob" 
                      remotecontext="$once{  canvas.applicationRPC }" >    
        <attribute name="diagramType" value="0" type="number" />
        <attribute name="diagramId" value="0" type="number" />
        <netparam><method name="getValue">return canvas.sessionId; </method></netparam>
        <netparam><method name="getValue">return parent.parent.baseactionobjectList;</method></netparam>
        <netparam><method name="getValue">return canvas.currentDiagramRef.currentDiagramTempName;</method></netparam>
        <netparam><method name="getValue">return parent.diagramId;</method></netparam>
        <netparam><method name="getValue">return parent.parent.diagramType;</method></netparam>
        <netparam><method name="getValue">return parent.parent.pTemplateId;</method></netparam>
        <netparam><method name="getValue">return parent.parent.includeText;</method></netparam>
        <netparam><method name="getValue">return parent.parent.printIdeas;</method></netparam>
        <netparam><method name="getValue">return parent.parent.includeDetails;</method></netparam>
        <handler name="ondata" args="value">
            <![CDATA[
                //if ($debug) Debug.write("generateExportJob: ",value);
                var downloadurl = canvas.httpprotocol+'://'+canvas.rtmphostlocal+':'+canvas.httpport
                        +'/'+canvas.webAppRootKey+'/ExportDiagram?sid='+canvas.sessionId
                        +'&exportId='+value+'&exportType='+parent.exportType;
                //if ($debug) Debug.write(downloadurl);
                lz.Browser.loadURL(downloadurl,'_parent');
            ]]>
        </handler>
    </netRemoteCallRPC>    
    
    
    <method name="showPreNextDiagramsOnFlows" args="bool">
    	<![CDATA[
	        //if ($debug) Debug.write("showPreNextDiagramsOnFlows ",bool);
        	if (bool) {
        		
        		this.getFlowConnectedDiagramList.inputFlowIds = new Array();
        		this.getFlowConnectedDiagramList.outputFlowIds = new Array();
        		
        		for (var i=0;i<this.baseactionobjectList.length;i++) {
                    //simulate the Role as a Fixed, as we got only one Role Type
                    if (this.baseactionobjectList[i][0] == 'inputflow') {
                        this.getFlowConnectedDiagramList.inputFlowIds.push(this.baseactionobjectList[i][this.baseactionobjectList[i].length-7]);
                    }
                    if (this.baseactionobjectList[i][0] == 'outputflow') {
                        this.getFlowConnectedDiagramList.outputFlowIds.push(this.baseactionobjectList[i][this.baseactionobjectList[i].length-7]);
                    }
                }
                
                //if ($debug) Debug.write("inputFlowIds: ",this.getFlowConnectedDiagramList.inputFlowIds);
                //if ($debug) Debug.write("outputFlowIds: ",this.getFlowConnectedDiagramList.outputFlowIds);
                
                this.getFlowConnectedDiagramList.singleItem = false;
                this.getFlowConnectedDiagramList.doCall();
        		
        	} else {
        		
        		for (var i=0;i<this.subviews.length;i++) {
        			if (this.subviews[i].typeOfObject == "inputflow" 
        			         || this.subviews[i].typeOfObject == "outputflow") {
			             this.subviews[i].clearFlowDiagramList();	
			         }
        		}
        		
        	}
    	]]>
    </method>
    
    <method name="showPreNextDiagramsOnCertainFlow" args="objRef">
    	this.getFlowConnectedDiagramList.inputFlowIds = new Array();
        this.getFlowConnectedDiagramList.outputFlowIds = new Array();
        
        var baseObject = this.getBaseObjectListItemByName(objRef.name);
        
        if (baseObject[0] == 'inputflow') {
            this.getFlowConnectedDiagramList.inputFlowIds.push(baseObject[baseObject.length-7]);
        }
        if (baseObject[0] == 'outputflow') {
            this.getFlowConnectedDiagramList.outputFlowIds.push(baseObject[baseObject.length-7]);
        }
        
        this.getFlowConnectedDiagramList.singleItem = true;
        this.getFlowConnectedDiagramList.objRef = objRef;
        this.getFlowConnectedDiagramList.doCall();
    </method>
    
    <!--
    public List<FlowConnectedDiagram> getFlowConnectedDiagramList(String SID, 
            Vector inputFlowIds, Vector outputFlowIds)
     -->
    <netRemoteCallRPC name="getFlowConnectedDiagramList" funcname="getFlowConnectedDiagramList" 
                      remotecontext="$once{  canvas.diagramServiceRPC }" >    
        <attribute name="inputFlowIds" value="null" />
        <attribute name="outputFlowIds" value="null" />
        <attribute name="singleItem" value="false" type="boolean" />
        <attribute name="objRef" value="null" />
        <netparam><method name="getValue">return canvas.sessionId; </method></netparam>
        <netparam><method name="getValue">return parent.inputFlowIds;</method></netparam>
        <netparam><method name="getValue">return parent.outputFlowIds;</method></netparam>
        <handler name="ondata" args="value">
            <![CDATA[
                //if ($debug) Debug.write("getFlowConnectedDiagramList: ",value);
                //for (var eg in value) {
                //	if ($debug) Debug.write("diagramObjectId: ",value[eg].diagramObjectId);
                //	if ($debug) Debug.write("diagrams Size: ",value[eg].diagrams.length);
                //	if ($debug) Debug.write("diagrams: ",value[eg].diagrams);
                //}
                if (singleItem) {
                    parent.setFlowTextNamesBySingleItem(value);
                } else {
                	parent.setFlowTextNames(value);
                }
            ]]>
        </handler>
    </netRemoteCallRPC>   
    
    <method name="setFlowTextNames" args="list">
    	<![CDATA[
    	   
            for (var eg in list) {
                
            	if ($debug) Debug.write("diagramObjectId: ",list[eg].diagramObjectId);
            	
            	var baseObjectList = this.getBaseObjectListItemsByDiagramId(list[eg].diagramObjectId);
            	
            	for (var i = 0;i<baseObjectList.length;i++) {
            		
            		//if ($debug) Debug.write("baseObjectList[i]: ",baseObjectList[i]);
            		
            		var viewItem = this[baseObjectList[i][baseObjectList[i].length-1]];
            		//if ($debug) Debug.write("viewItem: ",viewItem);
            		
            		viewItem.setFlowDiagramList(list[eg].diagrams);
            		
            	}
            	
            }
    	
    	]]>
    </method>
    
    <method name="setFlowTextNamesBySingleItem" args="list">
    	<![CDATA[
           
            for (var eg in list) {
                
                if ($debug) Debug.write("diagramObjectId: ",list[eg].diagramObjectId);
                
                this.getFlowConnectedDiagramList.objRef.setFlowDiagramList(list[eg].diagrams);
                
            }
        
        ]]>
    </method>
     
        
    
</class>

</library>
